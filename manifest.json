{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "1.1",
  "description": "__MSG_extDesc__",
  "default_locale": "en",
  "icons": {
    "16": "images/proxyfox-logo-16.png",
    "48": "images/proxyfox-logo-48.png",
    "128": "images/proxyfox-logo-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/proxyfox-logo-16.png",
      "48": "images/proxyfox-logo-48.png",
      "128": "images/proxyfox-logo-128.png"
    }
  },
  "options_page": "options.html",
  "permissions": [
    "proxy",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "js/background.js"
  }
} 